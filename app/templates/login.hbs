
<h1 class="cover-heading">Please login</h1>
<div id="mainLogg">
  <section>

    <form class="main-login-form">

    <table>
    <br>
      <tr>
        <td>
          <label>Username:</label>
          {{input value=username type="text" placeholder="Enter Username"}}
        </td>
      </tr>


    <br>

      <tr>
        <td>
          <label>Password:</label>
          {{input value=password type="password" placeholder="Enter Password"}}

        </td>
      </tr>

    <br>

    </table>

      <div class="btn btn-primary" {{action 'login'}}>Login</div>

    </form>

  </section>

  <hr>

  <section style="text-align=center;">
      <h2>Don't have an account yet?</h2>

      <div class="reg-btn">{{#link-to 'register' }}Register{{/link-to}}</div>

  </section>


</div>

  {{outlet}}
